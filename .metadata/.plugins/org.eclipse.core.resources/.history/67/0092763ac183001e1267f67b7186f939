package constructor;

public class Constructor {

	int x, y;
	String s;
	
	public Constructor() {
		
		x=100;
		y=50;
		s="Selenium";
	}
	
	public void display() {
		System.out.println(x);
		System.out.println(y);
		System.out.println(s);
	}
	

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Constructor con = new Constructor();
		con.display();
		
	}

}
